    .o. ["a1s+" -> {inw-}   || _ V ]
    .o. ["a1s+" -> {nu-}    // "come+" _ ]
    .o. ["a1s+" -> {in-}    || _ C ] 
    .o. ["a2s+" -> {a-}     || _ C ]
    .o. ["a2s+" -> {a-}     || _ C ]
    .o. ["a2s+" -> {aw-}    || _ V ]
