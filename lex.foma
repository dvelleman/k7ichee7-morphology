    .o. ["a2p+" -> {i-}     || _ C ]
    .o. ["a2p+" -> {iw-}    || _ V ] 
    .o. ["a3p+" -> {ki-}    || _ C ]
    .o. ["a3p+" -> {k-}     || _ V ] 
];

define makeNominalSetA [
        ["a1s+" -> {w-}     || _ V ]
    .o. ["a1s+" -> {nu-}    || _ C ]
    .o. makeVerbalSetA
